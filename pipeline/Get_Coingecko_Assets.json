{
	"name": "Get_Coingecko_Assets",
	"properties": {
		"activities": [
			{
				"name": "Get Coins",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET"
					},
					"sink": {
						"type": "JsonSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "JsonWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "CoingeckoAPI",
						"type": "DatasetReference",
						"parameters": {
							"relativeURL": "coins/list?include_platform=false"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ResultsJSON",
						"type": "DatasetReference",
						"parameters": {
							"container": "coingecko",
							"fileName": "coins.json"
						}
					}
				]
			},
			{
				"name": "Import Coins",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Get Coins",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "SqlServerSink",
						"preCopyScript": "--TRUNCATE TABLE cko.coins",
						"tableOption": "autoCreate"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['id']"
								},
								"sink": {
									"name": "id"
								}
							},
							{
								"source": {
									"path": "$['symbol']"
								},
								"sink": {
									"name": "symbol"
								}
							},
							{
								"source": {
									"path": "$['name']"
								},
								"sink": {
									"name": "name"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "ResultsJSON",
						"type": "DatasetReference",
						"parameters": {
							"container": "coingecko",
							"fileName": "coins.json"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "Sql_Cryptoanalysis",
						"type": "DatasetReference",
						"parameters": {
							"schema": "cko",
							"table": "coins"
						}
					}
				]
			}
		],
		"folder": {
			"name": "To BLOB"
		},
		"annotations": [],
		"lastPublishTime": "2021-03-11T11:50:49Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}